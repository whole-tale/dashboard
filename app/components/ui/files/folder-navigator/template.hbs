                            <div class="icon-wrapper">
                                <i class="{{nav.icon}} icon big"></i>  {{#if (and (not useIcons) (eq nav.name 'Data'))}}external_{{/if}}{{snake-case nav.name}} <span class="chevron-span"><i class="fas fa-2x fa-chevron-right"></i></span>
                            </div>
                        </a>
                    {{/if}}
                </div>
            {{/each}}
        </div>
    </div>
</div>
